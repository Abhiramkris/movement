  <script>
    // Define the toggleLiveChat function
    function toggleLiveChat() {
      var modal = document.getElementById('live-chat-modal');
      if (modal) {
        modal.classList.toggle('open');
      }
    }

    // Event listener for form submission
    document.addEventListener('DOMContentLoaded', function() {
      document.getElementById('live-chat-form').addEventListener('submit', function(event) {
        event.preventDefault(); // Prevent default form submission

        // Collect form data
        var name = document.getElementById('name').value;
        var email = document.getElementById('email').value;
        var phone = document.getElementById('phone').value;
        var question = document.getElementById('question').value;

        // Send form data to server using fetch
        fetch('/save-chat-query', {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json'
            },
            body: JSON.stringify({
              name: name,
              email: email,
              phone: phone,
              question: question
            })
          })
          .then(response => response.json())
          .then(data => {
            console.log('Form submission successful:', data);
            toggleLiveChat(); // Close the modal after successful submission
            alert("We will get to you soon");
          })
          .catch(error => {
            console.error('Error submitting form:', error);
            // Optionally, show an error message to the user
          });
      });
    });
  </script>


