  <script>
    let cardContainer = document.getElementById('card-container');
    let overlay = document.getElementById('overlay');
    let activeCard = null;

    function showCard(event, cardId) {
      event.preventDefault();

      if (activeCard) {
        activeCard.style.display = 'none';
      }

      const selectedCard = document.getElementById(`card-${cardId}`);
      selectedCard.style.display = 'block';
      cardContainer.style.display = 'block';
      overlay.style.display = 'block';

      setTimeout(() => {
        cardContainer.style.opacity = '1';
        cardContainer.style.transform = 'translate(-50%, -50%) scale(1)';
        overlay.style.opacity = '1';
      }, 100);

      activeCard = selectedCard;

      document.removeEventListener('click', handleClickOutside);
      setTimeout(() => {
        document.addEventListener('click', handleClickOutside);
      }, 100);
    }

    function closeCard() {
      if (activeCard) {
        activeCard.style.display = 'none';
      }
      cardContainer.style.opacity = '0';
      cardContainer.style.transform = 'translate(-50%, -50%) scale(0.9)';
      overlay.style.opacity = '0';

      setTimeout(() => {
        cardContainer.style.display = 'none';
        overlay.style.display = 'none';
        activeCard = null;
      }, 300);

      document.removeEventListener('click', handleClickOutside);
    }

    function handleClickOutside(event) {
      if (!cardContainer.contains(event.target) && activeCard) {
        closeCard();
      }
    }

    document.addEventListener('keydown', function(event) {
      if (event.key === 'Escape') {
        if (cardContainer.style.display === 'block') {
          closeCard();
        }
      }
    });
  </script>
