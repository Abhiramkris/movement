      <script>
        const texts = [
          "After surgery",
          "Aches and pain",
          "Mobility problems",
          "Musculoskeletal rehabilitation",
          "Sports injury",
          "Adults and Children",
          "Cardio respiratory condition",
          "Neurological rehabilitation",
          "Massage and soft tissue mobilisation"
        ];

        let currentIndex = 0;

        function typeWriter(text, i, fnCallback) {
          if (i < text.length) {
            document.getElementById("typewriter-text").innerHTML = text.substring(0, i + 1);
            setTimeout(function() {
              typeWriter(text, i + 1, fnCallback);
            }, 20); // Adjust typing speed
          } else if (typeof fnCallback === 'function') {
            setTimeout(fnCallback, 1000);
          }
        }

        function startTextAnimation() {
          if (currentIndex < texts.length) {
            typeWriter(texts[currentIndex], 0, function() {
              currentIndex++;
              startTextAnimation();
            });
          } else {
            currentIndex = 0; // Reset the index to loop
            startTextAnimation();
          }
        }

        startTextAnimation();
      </script>

