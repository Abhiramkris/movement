  <style>
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
      font-family: 'Poppins', sans-serif;
    }

    .container {
      display: grid;
      grid-template-columns: repeat(3, 1fr);
      gap: 60px;
      padding: 55px;
    }

    .box {
      position: relative;
      height: 200px;
      background-size: cover;
      background-position: center;
      border-radius: 10px;
      overflow: hidden;
    }

    .box::after {
      content: "";
      position: absolute;
      bottom: 0;
      left: 0;
      right: 0;
      height: 90%;
      background: linear-gradient(to bottom, rgba(0, 238, 255, 0), rgba(0, 36, 43, 0.825));
    }

    .buttons {
      position: absolute;
      bottom: 10px;
      left: 10px;
      right: 10px;
      display: flex;
      z-index: 9;
      margin: auto;
      width: 30%;
    }

    .button {
      background-color: #007bff;
      color: white;
      border: none;
      padding: 10px 30px;
      border-radius: 100px;
      cursor: pointer;
      text-decoration: none;
      text-align: center;
    }

    .button.call {
      background-color: #28a745;
      border-radius: 100px;
      margin-left: 10px;
      padding: 10px 10px;
    }

    .subhead3 {
      color: #ffffff;
      font-weight: 700;
      text-align: center;
      font-size: 18px;
      line-height: 30px;
      position: absolute;
      bottom: 70px;
      /* Adjust this value as needed */
      left: 0;
      right: 0;
      z-index: 10;
    }

    .how {
      background-color: #011d3b;
    }

    @media (max-width: 767px) {
      .container {
        display: block;
        width: 95%;
      }

      .box {
        margin-top: 40px;
      }

      .buttons {
        position: absolute;
        bottom: 10px;
        left: 10px;
        right: 10px;
        display: flex;
        z-index: 9;
        margin: auto;
        width: 30%;
      }
    }

    .herohero {
      margin-left: 100px;
      margin-top: 40px;
      width: 60%;
    }

    @media (max-width: 1200px) {
      .container {
        grid-template-columns: repeat(3, 1fr);
      }
    }

    @media (max-width: 1000px) {
      .container {
        grid-template-columns: repeat(2, 1fr);
      }
    }

    @media (max-width: 700px) {
      .herohero {
        margin-left: 20px;
        width: 90%;
      }
    }
  </style>
